{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/power-platform/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "titleSuffix": "Power Platform Admin center",
      "feedback_system": "None"
      }
    },
    "fileMetadata": {
      "searchScope": {
        "**.*": [
          "PowerApps",
          "Flow",
          "Power Platform"
        ]
      },
    "template": [],
    "dest": "power-platform",
    "markdownEngineName": "markdig"
  }
}